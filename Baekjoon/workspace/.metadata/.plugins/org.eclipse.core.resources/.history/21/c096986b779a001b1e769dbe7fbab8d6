import java.util.*;

public class Main {
	// 출처: https://fbtmdwhd33.tistory.com/71
	static int dp[];
	
	public static void main(String[] args)   {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		dp = new int[n+1];
		
		dp[1] =1;
		dp[2]=3;
		
		for(int i=3;i<=n;i++) {
			
			for(int j=1;j<=i-1;j++) {
				dp[i] += dp[i-j] + dp[j];
				System.out.println(i + " = " + (i-j) + " + " + j);
			}
			 
		}
		
		System.out.println(dp[n]);
		sc.close();
	}
	
}
