import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.IOException;
import java.io.InputStreamReader;
import java.io.OutputStreamWriter;

public class Main {

	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		BufferedWriter bw = new BufferedWriter(new OutputStreamWriter(System.out));

		int T = Integer.parseInt(br.readLine());
		
		
//		d[1] = 1밖에 없으니 경우의 수는 1
//		d[2] = (1 + 1), (2) 즉 경우의 수는 2
//		d[3] = (1 + 1 + 1), (1 + 2), (2 + 1), (3) 즉 경우의 수는 3
		
		int[] dp = new int[11];
		
		int[] question = new int[T];
		for(int i=0;i<T;i++) {
			question[i] = Integer.parseInt(br.readLine());
			
			
			for(int j=4;j<=question[i];j++) {
				dp[j] = dp[j-1] + dp[j-2] + dp[j-3];
			}
			bw.write(dp[question[i]]);
			System.out.println(dp[quesiont[i]]);
		}
		
		br.close();
		bw.flush();
		bw.close();
	}

}
