
public class Main {

	public static void main(String[] args) {
		/* 주어진 노드를 시작 노드로 DFS 탐색1 */
		Graph g1 = new Graph(4);

	    g1.addEdge(0, 1);
	    g1.addEdge(0, 2);
	    g1.addEdge(1, 2);
	    g1.addEdge(2, 0);
	    g1.addEdge(2, 3);
	    g1.addEdge(3, 3);

	    g1.startDFS(2); 
	    
	    /* 주어진 노드를 시작 노드로 DFS 탐색2 */
	    Graph g2 = new Graph(10);

	    g2.addEdge(0, 1);
	    g2.addEdge(0, 2);
	    g2.addEdge(1, 2);
	    g2.addEdge(2, 0);
	    g2.addEdge(2, 3);
	    g2.addEdge(3, 3);

	    g.startDFS(2); 
	}

}
